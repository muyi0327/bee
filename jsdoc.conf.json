{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.(js|vue)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
   "plugins": ["node_modules/jsdoc-vue"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}