{
    "presets": [
        ["@babel/preset-env", {
            "modules": false,
            "targets": {
                "browsers": [">0.25%", "not ie 11", "not op_mini all"]
            },
            "useBuiltIns": false,
            "forceAllTransforms": true
        }]
    ],
    "plugins": [
        "@babel/plugin-transform-modules-commonjs",
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-object-assign"
    ]
}
